# Variables d'environnement pour Netlify
# Copier ce fichier vers .env.production sur Netlify

# Version de Node.js (géré par netlify.toml)
NODE_VERSION=18

# Configuration PWA
VITE_PWA_ENABLED=true

# URL de base (sera remplacée par l'URL Netlify)
VITE_BASE_URL=https://matricxconsulting.netlify.app

# Configuration Calendly
VITE_CALENDLY_URL=https://calendly.com/enablermoney/new-meeting

# Optimisations build
VITE_BUILD_SOURCEMAP=false
VITE_BUILD_MINIFY=true

# Configuration de sécurité
VITE_SECURITY_HEADERS=true

# Analytics (à configurer si besoin)
# VITE_GA_ID=G-XXXXXXXXXX
# VITE_HOTJAR_ID=XXXXXXX